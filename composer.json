{
    "name": "floor9design/character-humaniser",
    "type": "library",
    "description": "Turns difficult strings into easy to read strings ideal for sharing",
    "keywords": [
        "floor9design",
        "string"
    ],
    "homepage": "https://github.com/floor9design-ltd/character-humaniser",
    "license": "MIT",
    "authors": [
        {
            "name": "Rick Morice",
            "email": "rick@floor9design.com",
            "homepage": "https://floor9design.com",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php" : ">=7.4"
    },
    "require-dev": {
        "floor9design/testing-tools": "^1.3",
        "mockery/mockery": "dev-master",
        "phpstan/phpstan": "^1.0.0",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Floor9design\\CharacterHumaniser\\": "src",
            "Floor9design\\CharacterHumaniser\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
